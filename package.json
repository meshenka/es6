{
  "name": "es6",
  "version": "1.0.0",
  "description": "a demo glup with ES6 support",
  "repository": {
    "type": "git",
    "url": "git@github.com:meshenka/es6.git"
  },
  "main": "gulpfile.babel.js",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-plugin-class-property": "^1.0.3",
    "eslint-plugin-react": "^6.10.0",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.42.0",
    "gulp": "^3.9.1",
    "gulp-uglify": "^1.5.2",
    "husky": "^0.13.2",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "stylefmt": "^5.3.0",
    "stylelint": "^7.9.0",
    "stylelint-order": "^0.4.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-stream": "^3.1.0"
  },
  "scripts": {
    "precommit": "npm-run-all fix:* && npm-run-all lint:* && npm run build",
    "prebuild": "mkdir -p build",
    "build": "webpack",
    "start": "npm run build",
    "lint:js": "eslint src",
    "lint:js:type": "flow; test $? -eq 0 -o $? -eq 2",
    "lint:style": "stylelint \"src/**/*.scss\" ; test $? -eq 0 -o $? -eq 2",
    "fix:style": "stylefmt --config .stylelintrc --recursive 'src/**/*.scss'",
    "dev": "webpack-dev-server"
  },
  "tags": [
    "es6",
    "build",
    "front-end"
  ],
  "keywords": [
    "test",
    "es6",
    "stage-2",
    "javacript",
    "ecmascript",
    "eslint",
    "webpack",
    "gulp"
  ],
  "author": "Sylvain Gogel <sylvain.gogel@gmail.com>",
  "license": "MIT"
}
